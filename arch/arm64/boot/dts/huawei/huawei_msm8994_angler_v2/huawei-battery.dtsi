&pmi8994_fg {
	qcom,ext-sense-type;
};

&pmi8994_charger {
	qcom,float-voltage-mv = <4300>;
};
&i2c_5 {
	smb1351-charger@1d {
		compatible = "qcom,smb1351-charger";
		reg = <0x1d>;
		qcom,parallel-charger;
		qcom,float-voltage-mv = <4350>;
		qcom,recharge-thresh-mv = <100>;
	};
};
